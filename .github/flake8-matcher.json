{
  "problemMatcher": [
    {
      "owner": "flake8-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+)\\sE\\d+\\s(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "flake8-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+)\\sW\\d+\\s(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    }
  ]
}